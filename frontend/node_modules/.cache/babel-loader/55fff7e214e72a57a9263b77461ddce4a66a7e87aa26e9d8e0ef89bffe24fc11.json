  loading.value = false\r\n      }\r\n    }\r\n\r\n    return {\r\n      loginForm,\r\n      rules,\r\n      loading,\r\n      handleLogin\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login-container {\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.login-card {\r\n  width: 400px;\r\n  padding: 20px;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 30px;\r\n  color: #409EFF;\r\n}\r\n</style> "],"mappings":"AAgCA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,SAAQ,QAAS,YAAW;AACrC,OAAOC,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIJ,SAAS,CAAC;IACzB,MAAMK,SAAQ,GAAIN,QAAQ,CAAC;MACzBO,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;IACD,MAAMC,OAAM,GAAIV,GAAG,CAAC,KAAK;IAEzB,MAAMW,KAAI,GAAI;MACZH,KAAK,EAAE,CACL;QAAEI,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,6BAA6B;QAAEC,OAAO,EAAE;MAAO,CAAC,EAC3E;QAAEC,IAAI,EAAE,OAAO;QAAEF,OAAO,EAAE,iCAAiC;QAAEC,OAAO,EAAE;MAAO,EAC9E;MACDL,QAAQ,EAAE,CACR;QAAEG,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,oCAAoC;QAAEC,OAAO,EAAE;MAAO,CAAC,EAClF;QAAEE,GAAG,EAAE,CAAC;QAAEH,OAAO,EAAE,qDAAqD;QAAEC,OAAO,EAAE;MAAO;IAE9F;IAEA,MAAMG,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9BP,OAAO,CAACQ,KAAI,GAAI,IAAG;MACnB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,IAAI,CAAC,uCAAuC,EAAEb,SAAS;QACpFc,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEH,QAAQ,CAACI,IAAI,CAACC,KAAK;QACjDlB,MAAM,CAACmB,IAAI,CAAC,YAAY;MAC1B,EAAE,OAAOC,KAAK,EAAE;QACdC,SAAS,CAACD,KAAK,CAAC,iCAAiC;MACnD,UAAU;QACRhB,OAAO,CAACQ,KAAI,GAAI,KAAI;MACtB;IACF;IAEA,OAAO;MACLX,SAAS;MACTI,KAAK;MACLD,OAAO;MACPO;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}